{{> comment}}

{{#if typeParameters}}
    <h4 class="tsd-type-parameters-title">Type parameters</h4>
    {{> typeParameters}}
{{/if}}

{{#if parameters}}
    <ul class="parameters">
        {{#each parameters}}
            {{#if comment}}
                {{#compact}}
                    <li>
                        {{#if flags.isRest}}<span class="tsd-signature-symbol">...</span>{{/if}}
                        {{name}}
                        {{#if defaultValue}}
                            <span class="tsd-signature-symbol">
                            &nbsp;=&nbsp;
                            {{defaultValue}}
                            </span>
                        {{/if}}

                        {{> comment}}

                        {{#if type.declaration}}
                            {{#with type.declaration}}
                                {{> parameter}}
                            {{/with}}
                        {{/if}}
                    </li>
                {{/compact}}
            {{/if}}
        {{/each}}
    </ul>
{{/if}}

{{#if type}}
    {{#if comment.returns}}
        Returns {{#markdown}}{{{comment.returns}}}{{/markdown}}
    {{/if}}

    {{#if type.declaration}}
        {{#with type.declaration}}
            {{> parameter}}
        {{/with}}
    {{/if}}
{{/if}}
