{{#if getSignature}}
    {{#with getSignature}}
        {{#compact}}
            <h3>
                <span>get</span>&nbsp;
                {{../name}}
                {{> member.signature.title hideName=true }}
            </h3>
            {{> member.signature.body }}
        {{/compact}}
    {{/with}}
{{/if}}
{{#if setSignature}}
    {{#with setSignature}}
        {{#compact}}
            <h3>
                <span>set</span>&nbsp;
                {{../name}}
                {{> member.signature.title hideName=true }}
            </h3>
            {{> member.signature.body }}
        {{/compact}}
    {{/with}}
{{/if}}
